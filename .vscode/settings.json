{
  "stylelint.enable": true,
  "css.validate": true,
  "scss.validate": true,
  "stylelint.validate": [
    "css",
    "scss"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "never"
  },
  "eslint.format.enable": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "prettier.enable": false
}